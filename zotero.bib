@article{schneider_cerebral_2012,
	title = {Cerebral motor function in very premature-at-birth adolescents: a brain stimulation exploration of kangaroo mother care effects},
	volume = {101},
	issn = {1651-2227},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/22734793},
	doi = {10.1111/j.1651-2227.2012.02770.x},
	shorttitle = {Cerebral motor function in very premature-at-birth adolescents},
	abstract = {Aim:  Given that prematurity has deleterious effects on brain networking development beyond childhood, the study explored whether an early intervention such as Kangaroo Mother Care ({KMC}) in very preterm preemies could have influenced brain motor function up to adolescence. Methods:  Transcranial magnetic stimulation ({TMS}) was applied over the primary motor cortex (M1) of 39 adolescents born very prematurely ({\textless}33 weeks' gestational age, 21 having received {KMC} after birth, 18 Controls with no {KMC}) and nine adolescents born at term ({\textgreater}37 weeks' gestational age, {\textgreater}2500 g) to assess the functional integrity of motor circuits in each hemisphere (motor planning) and between hemispheres (callosal function). Results:  All {TMS} outcomes were similar between {KMC} and term adolescents, with typical values as in healthy adults, and better than in Controls. {KMC} adolescents presented faster conduction times revealing more efficient M1 cell synchronization (p {\textless} 0.05) and interhemispheric transfer time (p {\textless} 0.0001), more frequent inhibitory processes with a better control between hemispheres (p {\textless} 0.0001). Conclusion:  The enhanced synchronization, conduction times and connectivity of cerebral motor pathways in the {KMC} group suggests that the Kangaroo Mother Care positively influenced the premature brain networks and synaptic efficacy up to adolescence.},
	pages = {1045--1053},
	number = {10},
	journaltitle = {Acta paediatrica (Oslo, Norway: 1992)},
	shortjournal = {Acta Paediatr.},
	author = {Schneider, Cyril and Charpak, Nathalie and Ruiz-Peláez, Juan G and Tessier, Réjean},
	urldate = {2012-09-11},
	date = {2012-10},
	pmid = {22734793}
}

@article{sasaki_local_2001,
	title = {Local and global attention are mapped retinotopically in human occipital cortex},
	volume = {98},
	issn = {0027-8424 ({PRINT}) 0027-8424 ({LINKING})},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/11172078},
	doi = {10.1073/pnas.98.4.2077},
	shorttitle = {Local and global attention are mapped retinotopically in human occipital cortex},
	abstract = {Clinical evidence suggests that control mechanisms for local and global attention are lateralized in the temporal-parietal cortex. However, in the human occipital (visual) cortex, the evidence for lateralized local/global attention is controversial. To clarify this matter, we used functional {MRI} to map activity in the human occipital cortex, during local and global attention, with sustained visual fixation. Data were analyzed in a flattened cortical format, relative to maps of retinotopy and spatial frequency peak tuning. Neither local nor global attention was lateralized in the occipital cortex. Instead, local attention and global attention appear to be special cases of visual spatial attention, which are mapped consistently with the maps of retinotopy and spatial frequency tuning, in multiple visual cortical areas.},
	pages = {2077--82},
	number = {4},
	journaltitle = {Proc Natl Acad Sci U S A},
	shortjournal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {Sasaki, Y. and Hadjikhani, N. and Fischl, B. and Liu, A. K. and Marrett, S. and Dale, A. M. and Tootell, R. B.},
	date = {2001-02-13}
}

