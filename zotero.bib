	shortjournal = {Psychiatry Res},
	author = {Cerasa, Antonio and Quattrone, Aldo and Gioia, Maria C. and Magariello, Angela and Muglia, Maria and Assogna, Francesca and Bernardini, Sergio and Caltagirone, Carlo and Bossù, Paola and Spalletta, Gianfranco},
