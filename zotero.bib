@article{ronan_intrinsic_2011,
	title = {{INTRINSIC} {CURVATURE}: A {MARKER} {OF} {MILLIMETER}-{SCALE} {TANGENTIAL} {CORTICO}-{CORTICAL} {CONNECTIVITY}?},
